		"chai": "^4.2.0",
		"mocha": "^7.1.2",
		"nyc": "^15.0.1",
		"tidyscript": "^1.0.1",
		"ts-node": "^8.9.1",
		"typemoq": "^2.1.0",
		"typescript": "^3.8.3"
