{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,

    "declaration": true,
    "declarationMap": true,

    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,

    "outDir": "dist/"
  },
  "include": ["src/"]
}
